{
	"name": "AiiSkin",
	"version": "0.0.1-beta",
	"author": [
		"[https://gitee.com/morikawa56 Aii-kisaraki]"
	],
	"url": "https://gitee.com/morikawa56/mediawiki-aiiskin",
	"descriptionmsg": "aiiskin-skin-desc",
	"namemsg": "skinname-aiiskin",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.38.0"
	},
	"license-name": "GPL-2.0-or-later",
	"AutoloadClasses": {
		"SkinAii": "SkinAii.php"
	},
	"Hooks": {
		"SkinTemplateNavigation::Universal": "SkinAii::onSkinTemplateNavigation"
	},
	"ValidSkinNames": {
		"aiiskin": {
			"class": "SkinAii",
			"args": [
				{
					"name": "aiiskin",
					"templateDirectory": "templates",
					"responsive": true,
					"messages": [
						"aiiskin-no-categories",
						"otherlanguages",
						"sitetitle",
						"search",
						"tagline"
					],
					"styles": [
						"codex-styles",
						"mediawiki.ui.icon",
						"mediawiki.ui.button",
						"skins.aiiskin.styles"
					],
					"scripts": [
						"skins.aiiskin"
					]
				}
			]
		}
	},
	"ConfigRegistry": {
		"aiiskin": "GlobalVarConfig::newInstance"
	},
	"config": {
		"AiiSkinSearchApi": {
			"value": false
		}
	},
	"MessagesDirs": {
		"AiiSkin": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.aiiskin.styles": {
			"class": "ResourceLoaderSkinModule",
			"features": {
				"normalize": true,
				"content-media": true,
				"content-tables": true,
				"content-links": true,
				"interface-category": true,
				"toc": false
			},
			"targets": [
				"desktop",
				"mobile"
			],
			"styles": [
				"resources/skin.less"
			]
		},
		"skins.aiiskin": {
			"targets": [
				"desktop",
				"mobile"
			],
			"dependencies": [ "vue", "wvui" ],
			"es6": true,
			"packageFiles": [
				"resources/skin.js",
				"resources/App.vue"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "AiiSkin"
	},
	"ResourceModuleSkinStyles": {},
	"manifest_version": 2
}
